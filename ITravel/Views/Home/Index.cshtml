@inject ITravel.Models.LanguageService lang
@{
    Layout = "/Views/Shared/_Layout.cshtml";
}

<!-- end nav -->
<section class="carousel slide" id="banner" data-bs-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <div class="banner-caption">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="hero-text">
                                <h3 class="animated fadeInDown">@lang.GetKey("explore")</h3>
                                <h1 class="animated fadeInUp">@lang.GetKey("start_planning")</h1>
                                <p class="animated fadeInUp">@lang.GetKey("itravel_text")</p>
                                <div class="banner-button-group">
                                    <a href="@Url.Action("Contact", "Contact")" class="btn btn-primary animated fadeInUp">@lang.GetKey("contact")</a>
                                    <a href="@Url.Action("Tour", "Tour")" class="btn btn-primary learn-more animated fadeInUp">@lang.GetKey("learn_more")</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <img class="tilt-img" src="~/images/banner/slide-1.png" alt="Slider Image">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="carousel-item">
            <div class="banner-caption">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="hero-text">
                                <h3 class="animated fadeInLeft">@lang.GetKey("explore")</h3>
                                <h1 class="animated fadeInLeft">@lang.GetKey("trusted_agency")</h1>
                                <p class="animated fadeInLeft">@lang.GetKey("itravel_text")</p>
                                <div class="banner-button-group">
                                    <a href="@Url.Action("Contact", "Contact")" class="btn btn-primary animated fadeInUp">@lang.GetKey("contact")</a>
                                    <a href="@Url.Action("Tour", "Tour")" class="btn btn-primary learn-more animated fadeInUp">@lang.GetKey("learn_more")</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <img class="tilt-img" src="~/images/banner/slide-2.png" alt="Slider Image">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="carousel-item">
            <div class="banner-caption">
                <div class="container">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="hero-text">
                                <h3 class="animated fadeInRight">@lang.GetKey("explore")</h3>
                                <h1 class="animated fadeInRight">@lang.GetKey("exclusive")</h1>
                                <p class="animated fadeInRight">@lang.GetKey("itravel_text")</p>
                                <div class="banner-button-group">
                                    <a href="@Url.Action("Contact", "Contact")" class="btn btn-primary animated fadeInUp">@lang.GetKey("contact")</a>
                                    <a href="@Url.Action("Tour", "Tour")" class="btn btn-primary learn-more animated fadeInUp">@lang.GetKey("learn_more")</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <img class="tilt-img" src="~/images/banner/slide-3.png" alt="Slider Image">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#banner" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#banner" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
        <div class="shape1 d-none d-lg-block">
            <img src="~/images/banner/banner-shape1.png" alt="">
        </div>
        <div class="shape2 d-none d-lg-block">
            <img src="~/images/banner/banner-shape2.png" alt="">
        </div>
        <div class="shape3 d-none d-lg-block">
            <img src="~/images/banner/banner-shape3.png" alt="">
        </div>
    </div>
</section>
<!-- end banner -->
<!-- start destination -->
