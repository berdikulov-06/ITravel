<!-- end destination -->
<!-- start about us -->
<section class="section-spacing about-us">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-lg-5">
                <div class="about-us-left" data-cues="slideInUp">
                    <h3 class="mt-n6">@lang.GetKey("about_company")</h3>
                    <p>@lang.GetKey("about_text_1")</p>
                    <div class="about-info" data-cues="slideInLeft">
                        <div class="d-flex">
                            <div class="flex-shrink-0">
                                <span class="align-self-start mr-3 media-icon">
                                    <i class="far fa-compass"></i>
                                </span>
                            </div>
                            <div class="flex-grow-1 ms-3">
                                <h5 class="mt-0">@lang.GetKey("quick_booking")</h5>
                                <p>@lang.GetKey("quick_booking_text")</p>
                            </div>
                        </div>
                        <div class="d-flex">
                            <div class="flex-shrink-0">
                                <span class="align-self-start mr-3 media-icon">
                                    <i class="fas fa-briefcase"></i>
                                </span>
                            </div>
                            <div class="flex-grow-1 ms-3">
                                <h5 class="mt-0">@lang.GetKey("favorable_prices")</h5>
                                <p>@lang.GetKey("favorable_prices_text")</p>
                            </div>
                        </div>
                        <div class="d-flex">
                            <div class="flex-shrink-0">
                                <span class="align-self-start mr-3 media-icon">
                                    <i class="fas fa-umbrella-beach"></i>
                                </span>
                            </div>
                            <div class="flex-grow-1 ms-3">
                                <h5 class="mt-0">@lang.GetKey("any_tours")</h5>
                                <p>@lang.GetKey("any_tours_text")</p>
                            </div>
                        </div>
                    </div>
                    <div class="cta-button">
                        <a href="@Url.Action("Booking", "Booking")" class="btn btn-primary">@lang.GetKey("booking_now")</a>
                    </div>
                </div>
            </div>
            <div class="col-md-12 col-lg-7">
                <div class="about-right">
                    <img class="tilt-img" src="~/images/about/1.png" alt="">
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end about us -->
<!-- start features -->
<section class="section-spacing features" style="background-image: url(@Url.Content("~/images/features/1.png")" );">
    <div class="container">
        <div class="row justify-content-end">
            <div class="col-md-7 col-lg-5" data-cues="slideInRight">
                <h2>@lang.GetKey("about_text_3")</h2>
                <p>@lang.GetKey("about_text_2")</p>
