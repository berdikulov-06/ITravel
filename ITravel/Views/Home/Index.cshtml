                    <a href="javascript:void(0)" data-bs-toggle="modal" data-bs-target="#video-modal">
                        <i class="fa fa-play"></i>
                    </a>
                </div>
                <div class="features-items" data-cues="slideInRight">
                    <div class="feature-item">
                        <h3>
                            <span class="counter">1</span>
                        </h3>
                        <h4 class="mb-0">@lang.GetKey("year_experiance")</h4>
                    </div>
                    <div class="feature-item">
                        <h3>
                            <span class="counter">100</span>%
                        </h3>
                        <h4 class="mb-0">@lang.GetKey("trusted_agency")</h4>
                    </div>
                    <div class="feature-item">
                        <h3>
                            <span class="counter">99</span>%
                        </h3>
                        <h4 class="mb-0">@lang.GetKey("travel_happinies")</h4>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- end features -->
